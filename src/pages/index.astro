---
import Layout from '~/layouts/PageLayout.astro';

// import Hero from '~/components/widgets/Hero.astro';
import HomeHero from '~/components/widgets/HomeHero.astro';
import TestimonialQuincy from '~/components/widgets/TestimonialQuincy.astro';
import Services from '~/components/widgets/Services.astro';
import Pricing from '~/components/widgets/Pricing.astro';
// import FAQ from '~/components/widgets/FAQ.astro';
import HomeFAQ from '~/components/widgets/HomeFAQ.astro';
import ContactSection from '~/components/widgets/ContactSection.astro';

const metadata = {
  title: 'Rock Bookkeeping - We handle your bookkeeping. You handle your business.',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>
  <HomeHero />
  <TestimonialQuincy />
  <Services bgColor="#FBFCFC" />
  <Pricing bgColor="#f7f9ff" />
  <!-- <FAQ bgColor="#f9fafb" /> -->
  <HomeFAQ bgColor="#f9fafb" />
  <ContactSection />
</Layout>
